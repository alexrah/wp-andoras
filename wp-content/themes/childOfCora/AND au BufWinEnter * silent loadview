setlocal define=
setlocal dictionary=
setlocal nodiff
setlocal equalprg=~/.vim/csstidy.php\ -\ -t\ default\ -l\ LF
setlocal errorformat=
setlocal expandtab
if &filetype != 'css'
setlocal filetype=css
endif
setlocal foldcolumn=0
setlocal nofoldenable
setlocal foldexpr=0
setlocal foldignore=#
setlocal foldlevel=0
setlocal foldmarker={{{,}}}
setlocal foldmethod=syntax
setlocal foldminlines=1
setlocal foldnestmax=20
setlocal foldtext=XMLFoldLabel()
setlocal formatexpr=
