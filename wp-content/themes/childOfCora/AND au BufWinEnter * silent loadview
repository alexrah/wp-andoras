let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
argglobal
edit /Applications/XAMPP/xamppfiles/htdocs/wp_andora/wp-content/themes/childOfCora/style.css
setlocal autoindent
setlocal nobinary
